<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:divider="@drawable/qmui_divider"
    android:dividerPadding="1dp"
    android:showDividers="end"
    android:theme="@style/AppThemeQmui"
    android:background="@drawable/qmui_s_list_item_bg_with_border_bottom"
    android:gravity="center_vertical">


    <TextView android:id="@+id/id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:textColor="@color/btn_ghost_blue_border_normal"
        android:paddingLeft="10dp"
        />

    <ImageView android:id="@+id/ivLogo"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/not"/>

    <RelativeLayout android:id="@+id/rl_name"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:layout_weight="1">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="1"
            android:useDefaultMargins="true">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:divider="@drawable/qmui_divider"
                android:dividerPadding="1dp"
                android:showDividers="middle"
                android:layout_weight="1"
                android:background="@drawable/qmui_s_list_item_bg_with_border_bottom"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:divider="@drawable/qmui_divider"
                    android:dividerPadding="1dp"
                    android:showDividers="middle"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvApplicationName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:paddingTop="10dp"
                        android:gravity="center_vertical"
                        android:textColor="?attr/qmui_config_color_gray_1"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:divider="@drawable/qmui_divider"
                android:dividerPadding="1dp"
                android:showDividers="middle"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:divider="@drawable/qmui_divider"
                    android:dividerPadding="1dp"
                    android:showDividers="middle"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvApplicationName2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:paddingBottom="10dp"
                        android:gravity="center_vertical"
                        android:textColor="?attr/qmui_config_color_gray_1"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size" />

                </LinearLayout>

            </LinearLayout>

        </GridLayout>


    </RelativeLayout>

    <RelativeLayout android:id="@+id/rc_name"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:layout_weight="1">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="1"
            android:useDefaultMargins="true">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:divider="@drawable/qmui_divider"
                android:dividerPadding="1dp"
                android:showDividers="middle"
                android:layout_weight="1"
                android:background="@drawable/qmui_s_list_item_bg_with_border_bottom"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:divider="@drawable/qmui_divider"
                    android:dividerPadding="1dp"
                    android:showDividers="middle"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvApplicationName3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:textColor="?attr/qmui_config_color_gray_1"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:divider="@drawable/qmui_divider"
                android:dividerPadding="1dp"
                android:showDividers="middle"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:divider="@drawable/qmui_divider"
                    android:dividerPadding="1dp"
                    android:showDividers="middle"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvApplicationName4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:textColor="?attr/qmui_config_color_gray_1"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size" />

                </LinearLayout>

            </LinearLayout>

        </GridLayout>


    </RelativeLayout>


    <RelativeLayout android:id="@+id/rr_name"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:layout_weight="1">

        <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="1"
            android:useDefaultMargins="true">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:divider="@drawable/qmui_divider"
                android:dividerPadding="1dp"
                android:showDividers="middle"
                android:layout_weight="1"
                android:background="@drawable/qmui_s_list_item_bg_with_border_bottom"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:divider="@drawable/qmui_divider"
                    android:dividerPadding="1dp"
                    android:showDividers="middle"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvApplicationName5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:textColor="?attr/qmui_config_color_gray_1"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:divider="@drawable/qmui_divider"
                android:dividerPadding="1dp"
                android:showDividers="middle"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:divider="@drawable/qmui_divider"
                    android:dividerPadding="1dp"
                    android:showDividers="middle"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvApplicationName6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:textColor="?attr/qmui_config_color_gray_1"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size" />

                </LinearLayout>

            </LinearLayout>

        </GridLayout>


    </RelativeLayout>

    <RelativeLayout android:id="@+id/rl2_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:layout_weight="0">

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:columnCount="1"
            android:useDefaultMargins="true">
            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:divider="@drawable/qmui_divider"
                android:dividerPadding="1dp"
                android:showDividers="middle"
                android:layout_weight="1"
                android:background="@drawable/qmui_s_list_item_bg_with_border_bottom"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:divider="@drawable/qmui_divider"
                    android:dividerPadding="1dp"
                    android:showDividers="middle"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvApplicationName7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="0"
                        android:gravity="center_vertical"
                        android:textColor="?attr/qmui_config_color_gray_1"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:divider="@drawable/qmui_divider"
                android:dividerPadding="1dp"
                android:showDividers="middle"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:divider="@drawable/qmui_divider"
                    android:dividerPadding="1dp"
                    android:showDividers="middle"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvApplicationName8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        android:textColor="?attr/qmui_config_color_gray_1"
                        android:textSize="?attr/qmui_common_list_item_title_h_text_size" />

                </LinearLayout>

            </LinearLayout>

        </GridLayout>

    </RelativeLayout>

</LinearLayout>


